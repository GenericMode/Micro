{
    "configurations": [
      {
        "name": "OrderAPI",
        "type": "coreclr",
        "request": "launch",
        "program": "${workspaceFolder}/OrderAPI/bin/Debug/net8.0/OrderAPI.dll",
        "cwd": "${workspaceFolder}/OrderAPI",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on: (https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      }
]
}